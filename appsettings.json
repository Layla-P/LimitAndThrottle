{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,    
    "GeneralRules": [
      {
        "Endpoint": "*/api/values",
        "Period": "10s",
        "Limit": 1
      },
      {
        "Endpoint": "*/api/values",
        "Period": "1m",
        "Limit": 5
      }
    ]
  }
}
